-- Scripts

local function COEW_fake_script() -- DaHoodGui.MainScript 
	local script = Instance.new('LocalScript', DaHoodGui)

	local UserInputService = game:GetService("UserInputService")
	local TweenService = game:GetService("TweenService")
	local CoreGui = game:GetService("CoreGui")
	
	local CanDrag = true
	
	local Dragging
	local DragInput
	local DragStart
	local StartPos
	
	local ScreenGui = script.Parent
	local MainFrame = ScreenGui.MainFrame
	local Title = MainFrame.TitleFrame.Title
	local OpenButton = ScreenGui.OpenButton
	
	TweenService:Create(MainFrame, TweenInfo.new(0.25), { Position = UDim2.fromScale(0.252, 0.157)}):Play()
	TweenService:Create(MainFrame, TweenInfo.new(0.25), { Size = UDim2.fromScale(0.495, 0.685) }):Play()
	
	local Sound = Instance.new("Sound", script)
	Sound.Name = "Click"
	Sound.SoundId = "rbxassetid://6895079853"
	
	game.StarterGui:SetCore("SendNotification", {
		Title = "Welcome!",
		Text = "Welcome to Da Hood Script! (Still in beta.)",
		Duration = 10,
		Button1 = "Okay"
	})
	
	local Target = nil
	
	local ButtonsActivated = {
		["Character"] = {
			SuperSpeed = false,
			SuperJump = false,
			HeadSit = false,
			AntiSlow = false,
			Fly = false
		},
		["Visuals"] = {
			ESP = false
		}
	}
	
	local function update(input)
		local delta = input.Position - DragStart
		local dragTime = 0.04
		local SmoothDrag = {}
		if CanDrag then
			SmoothDrag.Position = UDim2.new(StartPos.X.Scale, StartPos.X.Offset + delta.X, StartPos.Y.Scale, StartPos.Y.Offset + delta.Y)
			local dragSmoothFunction = TweenService:Create(MainFrame, TweenInfo.new(dragTime, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), SmoothDrag)
			dragSmoothFunction:Play()
		end
	end
	
	Title.InputBegan:Connect(function(Input)
		if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
			Dragging = true
			DragStart = Input.Position
			StartPos = MainFrame.Position
			Input.Changed:Connect(function()
				if Input.UserInputState == Enum.UserInputState.End then
					Dragging = false
				end
			end)
		end
	end)
	Title.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and Dragging and MainFrame.Size then
			update(input)
		end
	end)
	
	for _,PageButton in pairs(MainFrame.PagesFrame.Pages:GetChildren()) do
		if PageButton:IsA("TextButton") then
			PageButton.MouseButton1Click:Connect(function()
				script.Click:Play()
				for _,Page in pairs(MainFrame.FeaturesFrame:GetChildren()) do
					if Page:IsA("ScrollingFrame") then
						if Page.Name ~= PageButton.Name then
							Page.Visible = false
						else
							Page.Visible = true
						end
					end
				end
			end)
		end
	end
	
	for _,Page in pairs(MainFrame.FeaturesFrame:GetChildren()) do
		if Page:IsA("ScrollingFrame") then
			for _,PageFrame in pairs(Page:GetChildren()) do
				if PageFrame:IsA("Frame") then
					if PageFrame.Name == "SuperSpeed" then
						PageFrame:WaitForChild("Activate").MouseButton1Click:Connect(function()
							if ButtonsActivated.Character.SuperSpeed == false then
								Instance.new("IntValue", game.Players.LocalPlayer.Character.BodyEffects.Movement).Name = "SuperSpeed"
								ButtonsActivated.Character.SuperSpeed = true
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(120, 120, 120) }):Play()
								script.Click:Play()
	
								game.StarterGui:SetCore("SendNotification", {
									Title = "Activated",
									Text = "Super Speed activated",
									Duration = 10,
									Button1 = "Okay"
								})
							else
								game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("SuperSpeed"):Destroy()
								ButtonsActivated.Character.SuperSpeed = false
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(40, 40, 40) }):Play()
								script.Click:Play()
	
								game.StarterGui:SetCore("SendNotification", {
									Title = "Disabled",
									Text = "Super Speed disabled",
									Duration = 10,
									Button1 = "Okay"
								})
							end
						end)
					elseif PageFrame.Name == "SuperJump" then
						PageFrame:WaitForChild("Activate").MouseButton1Click:Connect(function()
							if ButtonsActivated.Character.SuperJump == false then
								Instance.new("IntValue", game.Players.LocalPlayer.Character.BodyEffects.Movement).Name = "HighJump"
								ButtonsActivated.Character.SuperJump = true
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(120, 120, 120) }):Play()
								script.Click:Play()
	
								game.StarterGui:SetCore("SendNotification", {
									Title = "Activated",
									Text = "Super Jump activated",
									Duration = 10,
									Button1 = "Okay"
								})
							else
								game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("HighJump"):Destroy()
								ButtonsActivated.Character.SuperJump = false
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(40, 40, 40) }):Play()
								script.Click:Play()
	
								game.StarterGui:SetCore("SendNotification", {
									Title = "Disabled",
									Text = "Super Jump disabled",
									Duration = 10,
									Button1 = "Okay"
								})
							end
						end)
					elseif PageFrame.Name == "TeleportTo" then
						PageFrame:WaitForChild("Activate").MouseButton1Click:Connect(function()
							if Target ~= nil then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Target.Character.HumanoidRootPart.CFrame
								script.Click:Play()
	
								game.StarterGui:SetCore("SendNotification", {
									Title = "Teleported",
									Text = "Teleported to target",
									Duration = 10,
									Button1 = "Okay"
								})
							end
						end)
					elseif PageFrame.Name == "TargetName" then
						PageFrame:WaitForChild("TextBox").FocusLost:Connect(function()
							for _,plr in pairs(game.Players:GetPlayers()) do
								if string.find(plr.Name, PageFrame.TextBox.Text) then
									Target = plr
									PageFrame.TextBox.Text = plr.Name
	
									game.StarterGui:SetCore("SendNotification", {
										Title = "Target set",
										Text = "Set target to: ".. plr.Name,
										Duration = 10,
										Button1 = "Okay"
									})
								elseif string.find(plr.DisplayName, PageFrame.TextBox.Text) then
									Target = plr
									PageFrame.TextBox.Text = plr.DisplayName
	
									game.StarterGui:SetCore("SendNotification", {
										Title = "Target set",
										Text = "Set target to: ".. plr.DisplayName,
										Duration = 10,
										Button1 = "Okay"
									})
								else
									game.StarterGui:SetCore("SendNotification", {
										Title = "Target set failed",
										Text = "Failed to set target, try entering username again!",
										Duration = 10,
										Button1 = "Okay"
									})
								end
							end
						end)
					elseif PageFrame.Name == "HeadSit" then
						PageFrame:WaitForChild("Activate").MouseButton1Click:Connect(function()
							if ButtonsActivated.Character.HeadSit == false then
								if Target ~= nil then
									ButtonsActivated.Character.HeadSit = true
									TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(120, 120, 120) }):Play()
									script.Click:Play()
	
									game.StarterGui:SetCore("SendNotification", {
										Title = "Activated",
										Text = "HeadSit activated",
										Duration = 10,
										Button1 = "Okay"
									})
								end
							else
								ButtonsActivated.Character.HeadSit = false
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(40, 40, 40) }):Play()
								script.Click:Play()
	
								game.StarterGui:SetCore("SendNotification", {
									Title = "Disabled",
									Text = "HeadSit disabled",
									Duration = 10,
									Button1 = "Okay"
								})
							end
						end)
					elseif PageFrame.Name == "AntiSlow" then
						PageFrame:WaitForChild("Activate").MouseButton1Click:Connect(function()
							if ButtonsActivated.Character.AntiSlow == false then
								ButtonsActivated.Character.AntiSlow = true
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(120, 120, 120) }):Play()
								script.Click:Play()
	
								game.StarterGui:SetCore("SendNotification", {
									Title = "Activated",
									Text = "Anti-Slow activated",
									Duration = 10,
									Button1 = "Okay"
								})
							else
								ButtonsActivated.Character.AntiSlow = false
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(40, 40, 40) }):Play()
								script.Click:Play()
	
								game.StarterGui:SetCore("SendNotification", {
									Title = "Disabled",
									Text = "Anti-Slow disabled",
									Duration = 10,
									Button1 = "Okay"
								})
							end
						end)
					elseif PageFrame.Name == "ESP" then
						PageFrame:WaitForChild("Activate").MouseButton1Click:Connect(function()
							if ButtonsActivated.Visuals.ESP == false then
								ButtonsActivated.Visuals.ESP = true
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(120, 120, 120) }):Play()
								script.Click:Play()
	
								game.StarterGui:SetCore("SendNotification", {
									Title = "Activated",
									Text = "ESP activated",
									Duration = 10,
									Button1 = "Okay"
								})
							else
								ButtonsActivated.Visuals.ESP = false
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(40, 40, 40) }):Play()
								script.Click:Play()
	
								game.StarterGui:SetCore("SendNotification", {
									Title = "Disabled",
									Text = "ESP disabled",
									Duration = 10,
									Button1 = "Okay"
								})
							end
						end)
					elseif PageFrame.Name == "Fly" then
						PageFrame:WaitForChild("Activate").MouseButton1Click:Connect(function()
							if ButtonsActivated.Character.Fly == false then
								ButtonsActivated.Character.Fly = true
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(120, 120, 120) }):Play()
								script.Click:Play()
								game.Players.LocalPlayer.Character.Animate.Disabled = true
								local bv = Instance.new("BodyVelocity", game.Players.LocalPlayer.Character.PrimaryPart)
								bv.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
								bv.Velocity = Vector3.new(0,0,0)
								bv.Name = "FlightForce"
	
								game.StarterGui:SetCore("SendNotification", {
									Title = "Activated",
									Text = "Fly activated",
									Duration = 10,
									Button1 = "Okay"
								})
							else
								ButtonsActivated.Character.Fly = false
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(40, 40, 40) }):Play()
								script.Click:Play()
								game.Players.LocalPlayer.Character.Animate.Disabled = false
								game.Players.LocalPlayer.Character.PrimaryPart:FindFirstChild("FlightForce"):Destroy()
	
								game.StarterGui:SetCore("SendNotification", {
									Title = "Disabled",
									Text = "Fly disabled",
									Duration = 10,
									Button1 = "Okay"
								})
							end
						end)
					end
				end
			end
		end
	end
	
	local OldPos = nil
	
	MainFrame.TitleFrame.CloseButton.MouseButton1Click:Connect(function()
		script.Click:Play()
		OpenButton.Visible = true
		CanDrag = false
		OldPos = MainFrame.Position
		TweenService:Create(MainFrame, TweenInfo.new(0.25), { Position = MainFrame.Position + UDim2.fromScale(MainFrame.Size.X.Scale/2, MainFrame.Size.Y.Scale/2) }):Play()
		TweenService:Create(MainFrame, TweenInfo.new(0.25), { Size = UDim2.fromScale(0, 0) }):Play()
	end)
	
	OpenButton.MouseButton1Click:Connect(function()
		script.Click:Play()
		OpenButton.Visible = false
		CanDrag = true
		TweenService:Create(MainFrame, TweenInfo.new(0.25), { Position = OldPos}):Play()
		TweenService:Create(MainFrame, TweenInfo.new(0.25), { Size = UDim2.fromScale(0.495, 0.685) }):Play()
	end)
	
	game.Players.LocalPlayer.CharacterAdded:Connect(function(Character)
		if ButtonsActivated.Character.SuperSpeed == true then
			Instance.new("IntValue", game.Players.LocalPlayer.Character.BodyEffects.Movement).Name = "SuperSpeed"
		end
		if ButtonsActivated.Character.SuperJump == true then
			Instance.new("IntValue", game.Players.LocalPlayer.Character.BodyEffects.Movement).Name = "HighJump"
		end
		if ButtonsActivated.Character.Fly == true then
			local bv = Instance.new("BodyVelocity", game.Players.LocalPlayer.Character.PrimaryPart)
			bv.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
			bv.Velocity = Vector3.new(0,0,0)
			bv.Name = "FlightForce"
		end
	end)
	
	local uis = game:GetService("UserInputService")
	
	local wPressed = false
	local sPressed = false
	local aPressed = false
	local dPressed = false
	
	if uis.LastInputTypeChanged ~= Enum.UserInputType.Touch then
		uis.InputBegan:Connect(function(key, chat)
			if chat then return end
	
			if key.KeyCode == Enum.KeyCode.W then
				wPressed = true
			elseif key.KeyCode == Enum.KeyCode.S then
				sPressed = true
			elseif key.KeyCode == Enum.KeyCode.A then
				aPressed = true
			elseif key.KeyCode == Enum.KeyCode.D then
				dPressed = true
			end
		end)
	
		uis.InputEnded:Connect(function(key)	
			if key.KeyCode == Enum.KeyCode.W then
				wPressed = false
			elseif key.KeyCode == Enum.KeyCode.S then
				sPressed = false
			elseif key.KeyCode == Enum.KeyCode.A then
				aPressed = false
			elseif key.KeyCode == Enum.KeyCode.D then
				dPressed = false
			end
		end)
	end
	
	local FlyFrame = ScreenGui.FlyFrame
	
	FlyFrame.Up.InputBegan:Connect(function(Input)
		wPressed = true
	end)
	FlyFrame.Up.InputEnded:Connect(function(Input)
		wPressed = false
	end)
	FlyFrame.Left.InputBegan:Connect(function(Input)
		aPressed = true
	end)
	FlyFrame.Left.InputEnded:Connect(function(Input)
		aPressed = false
	end)
	FlyFrame.Down.InputBegan:Connect(function(Input)
		sPressed = true
	end)
	FlyFrame.Down.InputEnded:Connect(function(Input)
		sPressed = false
	end)
	FlyFrame.Right.InputBegan:Connect(function(Input)
		dPressed = true
	end)
	FlyFrame.Right.InputEnded:Connect(function(Input)
		dPressed = false
	end)
	
	game:GetService("RunService").Heartbeat:Connect(function()
		if ButtonsActivated.Character.HeadSit == true then
			if Target ~= nil then
				game.Players.LocalPlayer.Character.Humanoid.Sit = true
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Target.Character.Head.CFrame
			end
		end
		if ButtonsActivated.Character.AntiSlow == true then
			if game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("NoWalkSpeed") then wait(0.1)
				game.Players.LocalPlayer.Character.BodyEffects.Movement.NoWalkSpeed:Destroy()
			elseif game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("ReduceWalk") then  wait(0.1)
				game.Players.LocalPlayer.Character.BodyEffects.Movement.ReduceWalk:Destroy()
			elseif game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("NoJumping") then wait(0.1)
				game.Players.LocalPlayer.Character.BodyEffects.Movement.NoJumping:Destroy() 
			end
		end
		
		local flyin = false
		local root = game.Players.LocalPlayer.Character.HumanoidRootPart
		local cam = game.Workspace.CurrentCamera
		root.Anchored = false
		
		FlyFrame.Visible = false
		
		if ButtonsActivated.Character.Fly == true then
			game.Players.LocalPlayer.Character.PrimaryPart:FindFirstChild("FlightForce").Velocity = Vector3.new(0,0,0)
			
			if uis:GetLastInputType() == Enum.UserInputType.Touch then
				FlyFrame.Visible = true
			end
			
			root.CFrame = CFrame.new(root.CFrame.p, root.CFrame.p + Vector3.new(cam.CFrame.LookVector.X, cam.CFrame.LookVector.Y, cam.CFrame.LookVector.Z))
			
			if wPressed then
				game.Players.LocalPlayer.Character.PrimaryPart:FindFirstChild("FlightForce").Velocity = workspace.CurrentCamera.CFrame.LookVector * 100
				flyin = true
			end
			if sPressed then
				game.Players.LocalPlayer.Character.PrimaryPart:FindFirstChild("FlightForce").Velocity = workspace.CurrentCamera.CFrame.LookVector * -100
				flyin = true
			end
			if aPressed then
				game.Players.LocalPlayer.Character.PrimaryPart:FindFirstChild("FlightForce").Velocity = workspace.CurrentCamera.CFrame.RightVector * -100
				flyin = true
			end
			if dPressed then
				game.Players.LocalPlayer.Character.PrimaryPart:FindFirstChild("FlightForce").Velocity = workspace.CurrentCamera.CFrame.RightVector * 100
				flyin = true
			end
		end
		if flyin == true then
			root.Anchored = false
		else
			if ButtonsActivated.Character.Fly == true then
				root.Anchored = true
			end
		end
		
		if ButtonsActivated.Visuals.ESP == true then
			for _,plr in pairs(game.Players:GetPlayers()) do
				if plr ~= game.Players.LocalPlayer then
					local char = plr.Character
	
					if not char:FindFirstChild("ESP") then
						local ESP = Instance.new("Highlight", char)
						ESP.Name = "ESP"
						ESP.OutlineTransparency = 1
					end
				end
			end
		elseif ButtonsActivated.Visuals.ESP == false then
			for _,plr in pairs(game.Players:GetPlayers()) do
				if plr ~= game.Players.LocalPlayer then
					local char = plr.Character
	
					if not char:WaitForChild("ESP") then
						char:WaitForChild("ESP"):Destroy()
					end
				end
			end
		end
	end)
	
	game.Players.PlayerRemoving:Connect(function(plr)
		if plr == Target then
			Target = nil
			
			game.StarterGui:SetCore("SendNotification", {
				Title = "Target left",
				Text = "Targetted player has left the server",
				Duration = 10,
				Button1 = "Okay"
			})
		end
	end)
end
coroutine.wrap(COEW_fake_script)()
