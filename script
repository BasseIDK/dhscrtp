-- Scripts

local function RWVZE_fake_script() -- DaHoodGui.MainScript 
	local script = Instance.new('LocalScript', DaHoodGui)

	local UserInputService = game:GetService("UserInputService")
	local TweenService = game:GetService("TweenService")
	local CoreGui = game:GetService("CoreGui")
	
	local CanDrag = true
	
	local Dragging
	local DragInput
	local DragStart
	local StartPos
	
	local ScreenGui = script.Parent
	local MainFrame = ScreenGui.MainFrame
	local Title = MainFrame.TitleFrame.Title
	local OpenButton = ScreenGui.OpenButton
	
	local Sound = Instance.new("Sound", script)
	Sound.Name = "Click"
	Sound.SoundId = "rbxassetid://6895079853"
	
	game.StarterGui:SetCore("SendNotification", {
		Title = "Welcome!",
		Text = "Welcome to Da Hood Script! (Still in development.)",
		Duration = 10,
		Button1 = "Okay"
	})
	
	local function Check(Char)
		for i,v in pairs(Char:GetDescendants()) do
			if v.Name == "LocalScript" and v.Parent.Parent == Char then
				v:Destroy()
			end
		end
	end
	
	Check(game.Players.LocalPlayer.Character)
	
	game.Players.LocalPlayer.CharacterAdded:Connect(function(Char)
		Char:WaitForChild("Humanoid")
		Check(Char)
	end)
	
	MainFrame.Position = UDim2.fromScale(0.5, 0.5)
	MainFrame.Size = UDim2.fromScale(0, 0)
	
	TweenService:Create(MainFrame, TweenInfo.new(0.25), { Position = UDim2.fromScale(0.252, 0.157)}):Play()
	TweenService:Create(MainFrame, TweenInfo.new(0.25), { Size = UDim2.fromScale(0.495, 0.685) }):Play()
	
	local Target = nil
	
	local ButtonsActivated = {
		["Character"] = {
			SuperSpeed = false,
			SuperJump = false,
			HeadSit = false,
			AntiSlow = false,
		},
		["Visuals"] = {
			ESP = false
		},
		["Notifications"] = {
			Show = true
		}
	}
	
	local function update(input)
		local delta = input.Position - DragStart
		local dragTime = 0.04
		local SmoothDrag = {}
		if CanDrag then
			SmoothDrag.Position = UDim2.new(StartPos.X.Scale, StartPos.X.Offset + delta.X, StartPos.Y.Scale, StartPos.Y.Offset + delta.Y)
			local dragSmoothFunction = TweenService:Create(MainFrame, TweenInfo.new(dragTime, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), SmoothDrag)
			dragSmoothFunction:Play()
		end
	end
	
	Title.InputBegan:Connect(function(Input)
		if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
			Dragging = true
			DragStart = Input.Position
			StartPos = MainFrame.Position
			Input.Changed:Connect(function()
				if Input.UserInputState == Enum.UserInputState.End then
					Dragging = false
				end
			end)
		end
	end)
	Title.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and Dragging and MainFrame.Size then
			update(input)
		end
	end)
	
	for _,PageButton in pairs(MainFrame.PagesFrame.Pages:GetChildren()) do
		if PageButton:IsA("TextButton") then
			PageButton.MouseButton1Click:Connect(function()
				script.Click:Play()
				for _,Page in pairs(MainFrame.FeaturesFrame:GetChildren()) do
					if Page:IsA("ScrollingFrame") then
						if Page.Name ~= PageButton.Name then
							Page.Visible = false
						else
							Page.Visible = true
							Title.Parent.CurrentPage.Text = Page.Name
						end
					end
				end
			end)
		end
	end
	
	for _,Page in pairs(MainFrame.FeaturesFrame:GetChildren()) do
		if Page:IsA("ScrollingFrame") then
			for _,PageFrame in pairs(Page:GetChildren()) do
				if PageFrame:IsA("Frame") then
					if PageFrame.Name == "SuperSpeed" then
						PageFrame:WaitForChild("Activate").MouseButton1Click:Connect(function()
							if ButtonsActivated.Character.SuperSpeed == false then
								Instance.new("IntValue", game.Players.LocalPlayer.Character.BodyEffects.Movement).Name = "SuperSpeed"
								ButtonsActivated.Character.SuperSpeed = true
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(120, 120, 120) }):Play()
								script.Click:Play()
	
								if ButtonsActivated.Notifications.Show == true then
								game.StarterGui:SetCore("SendNotification", {
									Title = "Activated",
									Text = "Super Speed activated",
									Duration = 10,
									Button1 = "Okay"
									})
									end
							else
								game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("SuperSpeed"):Destroy()
								ButtonsActivated.Character.SuperSpeed = false
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(40, 40, 40) }):Play()
								script.Click:Play()
	
								if ButtonsActivated.Notifications.Show == true then
								game.StarterGui:SetCore("SendNotification", {
									Title = "Disabled",
									Text = "Super Speed disabled",
									Duration = 10,
									Button1 = "Okay"
									})
									end
							end
						end)
					elseif PageFrame.Name == "SuperJump" then
						PageFrame:WaitForChild("Activate").MouseButton1Click:Connect(function()
							if ButtonsActivated.Character.SuperJump == false then
								Instance.new("IntValue", game.Players.LocalPlayer.Character.BodyEffects.Movement).Name = "HighJump"
								ButtonsActivated.Character.SuperJump = true
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(120, 120, 120) }):Play()
								script.Click:Play()
	
								if ButtonsActivated.Notifications.Show == true then
								game.StarterGui:SetCore("SendNotification", {
									Title = "Activated",
									Text = "Super Jump activated",
									Duration = 10,
									Button1 = "Okay"
									})
									end
							else
								game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("HighJump"):Destroy()
								ButtonsActivated.Character.SuperJump = false
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(40, 40, 40) }):Play()
								script.Click:Play()
	
								if ButtonsActivated.Notifications.Show == true then
								game.StarterGui:SetCore("SendNotification", {
									Title = "Disabled",
									Text = "Super Jump disabled",
									Duration = 10,
									Button1 = "Okay"
									})
									end
							end
						end)
					elseif PageFrame.Name == "TeleportTo" then
						PageFrame:WaitForChild("Activate").MouseButton1Click:Connect(function()
							if Target ~= nil then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Target.Character.HumanoidRootPart.CFrame
								script.Click:Play()
	
								if ButtonsActivated.Notifications.Show == true then
								game.StarterGui:SetCore("SendNotification", {
									Title = "Teleported",
									Text = "Teleported to target",
									Duration = 10,
									Button1 = "Okay"
									})
									end
							end
						end)
					elseif PageFrame.Name == "TargetName" then
						PageFrame:WaitForChild("TextBox").FocusLost:Connect(function()
							for _,plr in pairs(game.Players:GetPlayers()) do
								if string.find(plr.Name, PageFrame.TextBox.Text) then
									Target = plr
									PageFrame.TextBox.Text = plr.Name
	
									if ButtonsActivated.Notifications.Show == true then
									game.StarterGui:SetCore("SendNotification", {
										Title = "Target set",
										Text = "Set target to: ".. plr.Name,
										Duration = 10,
										Button1 = "Okay"
										})
										end
								elseif string.find(plr.DisplayName, PageFrame.TextBox.Text) then
									Target = plr
									PageFrame.TextBox.Text = plr.DisplayName
	
									if ButtonsActivated.Notifications.Show == true then
									game.StarterGui:SetCore("SendNotification", {
										Title = "Target set",
										Text = "Set target to: ".. plr.DisplayName,
										Duration = 10,
										Button1 = "Okay"
										})
										end
								else
									if ButtonsActivated.Notifications.Show == true then
									game.StarterGui:SetCore("SendNotification", {
										Title = "Target set failed",
										Text = "Failed to set target, try entering username again!",
										Duration = 10,
										Button1 = "Okay"
										})
										end
								end
							end
						end)
					elseif PageFrame.Name == "HeadSit" then
						PageFrame:WaitForChild("Activate").MouseButton1Click:Connect(function()
							if ButtonsActivated.Character.HeadSit == false then
								if Target ~= nil then
									ButtonsActivated.Character.HeadSit = true
									TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(120, 120, 120) }):Play()
									script.Click:Play()
	
									if ButtonsActivated.Notifications.Show == true then
									game.StarterGui:SetCore("SendNotification", {
										Title = "Activated",
										Text = "HeadSit activated",
										Duration = 10,
										Button1 = "Okay"
										})
										end
								end
							else
								ButtonsActivated.Character.HeadSit = false
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(40, 40, 40) }):Play()
								script.Click:Play()
	
								if ButtonsActivated.Notifications.Show == true then
								game.StarterGui:SetCore("SendNotification", {
									Title = "Disabled",
									Text = "HeadSit disabled",
									Duration = 10,
									Button1 = "Okay"
									})
									end
							end
						end)
					elseif PageFrame.Name == "AntiSlow" then
						PageFrame:WaitForChild("Activate").MouseButton1Click:Connect(function()
							if ButtonsActivated.Character.AntiSlow == false then
								ButtonsActivated.Character.AntiSlow = true
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(120, 120, 120) }):Play()
								script.Click:Play()
	
								if ButtonsActivated.Notifications.Show == true then
								game.StarterGui:SetCore("SendNotification", {
									Title = "Activated",
									Text = "Anti-Slow activated",
									Duration = 10,
									Button1 = "Okay"
									})
									end
							else
								ButtonsActivated.Character.AntiSlow = false
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(40, 40, 40) }):Play()
								script.Click:Play()
	
								if ButtonsActivated.Notifications.Show == true then
								game.StarterGui:SetCore("SendNotification", {
									Title = "Disabled",
									Text = "Anti-Slow disabled",
									Duration = 10,
									Button1 = "Okay"
									})
									end
							end
						end)
					elseif PageFrame.Name == "ESP" then
						PageFrame:WaitForChild("Activate").MouseButton1Click:Connect(function()
							if ButtonsActivated.Visuals.ESP == false then
								ButtonsActivated.Visuals.ESP = true
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(120, 120, 120) }):Play()
								script.Click:Play()
	
								if ButtonsActivated.Notifications.Show == true then
								game.StarterGui:SetCore("SendNotification", {
									Title = "Activated",
									Text = "ESP activated",
									Duration = 10,
									Button1 = "Okay"
									})
									end
							else
								ButtonsActivated.Visuals.ESP = false
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(40, 40, 40) }):Play()
								script.Click:Play()
	
								if ButtonsActivated.Notifications.Show == true then
								game.StarterGui:SetCore("SendNotification", {
									Title = "Disabled",
									Text = "ESP disabled",
									Duration = 10,
									Button1 = "Okay"
									})
									end
							end
						end)
					elseif PageFrame.Name == "Notifications" then
						PageFrame:WaitForChild("Activate").MouseButton1Click:Connect(function()
							if ButtonsActivated.Notifications.Show == false then
								ButtonsActivated.Notifications.Show = true
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(120, 120, 120) }):Play()
								script.Click:Play()
	
								game.StarterGui:SetCore("SendNotification", {
									Title = "Activated",
									Text = "Notifications enabled",
									Duration = 10,
									Button1 = "Okay"
								})
							else
								ButtonsActivated.Notifications.Show = false
								TweenService:Create(PageFrame.Activate, TweenInfo.new(0.25), { BackgroundColor3 = Color3.fromRGB(40, 40, 40) }):Play()
								script.Click:Play()
	
								game.StarterGui:SetCore("SendNotification", {
									Title = "Disabled",
									Text = "Notifications disabled",
									Duration = 10,
									Button1 = "Okay"
								})
							end
						end)
					end
				end
			end
		end
	end
	
	local OldPos = nil
	
	MainFrame.TitleFrame.CloseButton.MouseButton1Click:Connect(function()
		script.Click:Play()
		OpenButton.Visible = true
		CanDrag = false
		OldPos = MainFrame.Position
		TweenService:Create(MainFrame, TweenInfo.new(0.25), { Position = MainFrame.Position + UDim2.fromScale(MainFrame.Size.X.Scale/2, MainFrame.Size.Y.Scale/2) }):Play()
		TweenService:Create(MainFrame, TweenInfo.new(0.25), { Size = UDim2.fromScale(0, 0) }):Play()
	end)
	
	OpenButton.MouseButton1Click:Connect(function()
		script.Click:Play()
		OpenButton.Visible = false
		CanDrag = true
		TweenService:Create(MainFrame, TweenInfo.new(0.25), { Position = OldPos}):Play()
		TweenService:Create(MainFrame, TweenInfo.new(0.25), { Size = UDim2.fromScale(0.495, 0.685) }):Play()
	end)
	
	game.Players.LocalPlayer.CharacterAdded:Connect(function(Character)
		if ButtonsActivated.Character.SuperSpeed == true then
			Instance.new("IntValue", game.Players.LocalPlayer.Character.BodyEffects.Movement).Name = "SuperSpeed"
		end
		if ButtonsActivated.Character.SuperJump == true then
			Instance.new("IntValue", game.Players.LocalPlayer.Character.BodyEffects.Movement).Name = "HighJump"
		end
	end)
	
	game:GetService("RunService").Heartbeat:Connect(function()
		if ButtonsActivated.Character.HeadSit == true then
			if Target ~= nil then
				game.Players.LocalPlayer.Character.Humanoid.Sit = true
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Target.Character.Head.CFrame
			end
		end
		if ButtonsActivated.Character.AntiSlow == true then
			if game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("NoWalkSpeed") then wait(0.1)
				game.Players.LocalPlayer.Character.BodyEffects.Movement.NoWalkSpeed:Destroy()
			elseif game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("ReduceWalk") then  wait(0.1)
				game.Players.LocalPlayer.Character.BodyEffects.Movement.ReduceWalk:Destroy()
			elseif game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("NoJumping") then wait(0.1)
				game.Players.LocalPlayer.Character.BodyEffects.Movement.NoJumping:Destroy() 
			end
		end
		
		if ButtonsActivated.Visuals.ESP == true then
			for _,plr in pairs(game.Players:GetPlayers()) do
				if plr ~= game.Players.LocalPlayer then
					local char = plr.Character
	
					if not char.HumanoidRootPart:FindFirstChild("ESP") then
						local ESP = Instance.new("BillboardGui", char.HumanoidRootPart)
						ESP.Name = "ESP"
						ESP.Size = UDim2.new(0.5, 10, 0.5, 10)
						ESP.AlwaysOnTop = true
						
						local Frame = Instance.new("Frame", ESP)
						Frame.Size = UDim2.fromScale(1, 1)
						Frame.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
						
						local Corner = Instance.new("UICorner", Frame)
						Corner.CornerRadius = UDim.new(1, 0)
					end
				end
			end
		else
			for _,plr in pairs(game.Players:GetPlayers()) do
				if plr ~= game.Players.LocalPlayer then
					local char = plr.Character
	
					if char.HumanoidRootPart:FindFirstChild("ESP") then
						char.HumanoidRootPart.ESP:Destroy()
					end
				end
			end
		end
	end)
	
	game.Players.PlayerRemoving:Connect(function(plr)
		if plr == Target then
			Target = nil
			
			if ButtonsActivated.Notifications.Show == true then
				game.StarterGui:SetCore("SendNotification", {
					Title = "Target left",
					Text = "Targetted player has left the server",
					Duration = 10,
					Button1 = "Okay"
				})
			end
		end
	end)
end
coroutine.wrap(RWVZE_fake_script)()
